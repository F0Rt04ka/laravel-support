{
  "name": "efureev/laravel-support",
  "description": "PHP Support Package for Laravel",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "php",
    "support",
    "helpers",
    "laravel"
  ],
  "authors": [
    {
      "name": "Eugene Fureev",
      "homepage": "https://github.com/efureev"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "efureev/support": "^2.5",
    "illuminate/database": "^6.11 | ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.5",
    "orchestra/testbench": "^4.5",
    "squizlabs/php_codesniffer": "^3.5",
    "feugene/php-cs-fixer": "^1.0",
    "fzaninotto/faker": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "Php\\Support\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Php\\Support\\Laravel\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "phpcs": "@php ./vendor/bin/phpcs",
    "cs-fix": "@php ./vendor/bin/phpcbf",
    "phpunit": "@php ./vendor/bin/phpunit --no-coverage --testdox --colors=always",
    "phpunit-cover": "@php ./vendor/bin/phpunit --coverage-text",
    "test": [
      "@phpcs",
      "@phpunit"
    ],
    "test-cover": [
      "@phpcs",
      "@phpunit-cover"
    ]
  }
}
